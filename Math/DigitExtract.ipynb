    "nums = []\n",
    "def digitextract(num):\n",
    "    count = 0\n",
    "    while num > 0:\n",
    "        nums.append(num % 10)\n",
    "        num = num // 10\n",
    "        count = count + 1\n",
    "    nums.reverse()\n",
    "    return nums\n"
   ]
